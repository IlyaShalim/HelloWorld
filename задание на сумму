let str = [];
let num;
while (true) {
    let d = confirm("Zapustit programmu?");
    if (d === false) {
        break;
    } else {
        summArr()
    };
}
function summArr() {
    let sum = 0;
    while (true) {
        let fun = prompt("Vedite 4islo")
        if (fun == "" || fun == null) break;
        str.push(+fun);
    }
    for (let num of str) {
        sum = sum + num;
    }
    alert(sum);
    
}
